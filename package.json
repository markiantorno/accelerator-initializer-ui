{
  "name": "accelerator-initializer-ui",
  "version": "1.0.0",
  "description": "UI project for Accelerator Initializer application",
  "engines": {
    "node": "8.9.1",
    "npm": "5.5.1"
  },
  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server --config webpack.dev.config.js",
    "build": "npm run lint && npm run coverage && webpack --config webpack.prod.config.js",
    "test": "cross-env NODE_ENV=test mocha --recursive test_helper.js test --require mock-local-storage",
    "debug-test": "karma start",
    "lint": "eslint ./src",
    "coverage": "cross-env NODE_ENV=test nyc npm run test",
    "postinstall": "cross-var ./node_modules/.bin/check-node-version --node $npm_package_engines_node --npm $npm_package_engines_npm"
  },
  "nyc": {
    "require": [
      "babel-register"
    ],
    "all": true,
    "sourceMap": false,
    "instrument": false,
    "include": [
      "src"
    ],
    "exclude": [
      "src/index.js",
      "src/component/connect",
      "src/api",
      "src/helper"
    ],
    "reporter": [
      "lcov",
      "text"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/scotiabank/accelerator-initializer-ui.git"
  },
  "keywords": [
    "scotiabank",
    "accelerator",
    "initializer"
  ],
  "author": "Scotiabank",
  "license": "MPL-2.0",
  "dependencies": {
    "axios": "0.17.1",
    "history": "4.7.2",
    "intl": "1.2.5",
    "react": "~16.2.0",
    "react-dom": "16.2.1",
    "react-intl": "2.5.0",
    "react-redux": "5.0.6",
    "react-router-dom": "4.3.1",
    "react-router-redux": "5.0.0-alpha.8",
    "redux": "3.7.2",
    "redux-promise-middleware": "5.0.0",
    "redux-thunk": "2.2.0",
    "semantic-ui-less": "2.3.3",
    "semantic-ui-react": "0.77.2"
  },
  "devDependencies": {
    "babel-core": "6.26.0",
    "babel-eslint": "8.2.6",
    "babel-loader": "7.1.5",
    "babel-plugin-istanbul": "4.1.5",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "babel-preset-env": "1.6.1",
    "babel-preset-react": "6.24.1",
    "chai": "4.1.2",
    "check-node-version": "3.1.1",
    "clean-webpack-plugin": "0.1.17",
    "cross-env": "5.2.0",
    "cross-var": "1.1.0",
    "css-loader": "0.28.7",
    "enzyme": "3.6.0",
    "enzyme-adapter-react-16": "1.5.0",
    "eslint": "4.12.1",
    "eslint-plugin-react": "7.11.1",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "1.1.5",
    "html-loader": "0.5.1",
    "html-webpack-plugin": "2.30.1",
    "jsdom": "11.12.0",
    "karma": "3.1.1",
    "karma-chrome-launcher": "2.2.0",
    "karma-mocha": "1.3.0",
    "karma-webpack": "2.0.9",
    "less": "2.7.3",
    "less-loader": "4.0.5",
    "mocha": "4.0.1",
    "mock-local-storage": "1.0.5",
    "nyc": "11.3.0",
    "react-router": "4.3.1",
    "redux-mock-store": "1.5.3",
    "sinon": "4.1.3",
    "style-loader": "0.19.0",
    "uglifyjs-webpack-plugin": "1.3.0",
    "webpack": "3.10.0",
    "webpack-dev-server": "2.11.3",
    "webpack-merge": "4.1.4"
  }
}
